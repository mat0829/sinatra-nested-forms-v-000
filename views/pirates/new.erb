<h1>Pirate</h1>

  <form action="/pirate" method="post">
      Pirate Name: <input type="text" name="pirate[name]">
      Pirate Weight: <input type="text" name="pirate[weight]">
      Pirate Height: <input type="text" name="pirate[height]">
      Ship Name: <input type="text" name="pirate[ship][][name]">
      Ship Type: <input type="text" name="pirate[ship][][type]">
      Ship Booty: <input type="text" name="pirate[ship][][booty]">
      <input type="submit">
  </form> 

